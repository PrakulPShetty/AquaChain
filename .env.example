# Example environment variables for AquaChain

# Backend
PORT=5000
FRONTEND_ORIGIN=http://localhost:3000

# PostgreSQL
# Either use a full DATABASE_URL, or PG* variables will be used to build it
DATABASE_URL=
PGHOST=localhost
PGPORT=5432
PGUSER=app
PGPASSWORD=app_password
PGDATABASE=aquachain

# Blockchain (fill in when deploying)
SEPOLIA_RPC=https://ethereum-sepolia-rpc.publicnode.com
DEPLOYER_KEY=0x2df4aa53b5a80acd41ebacbb09bad35743158e53803f3fff13e6f7feeef88b8d
PORT=3000
CORS_ORIGIN=http://localhost:5173

# Database (PostgreSQL)
POSTGRES_USER=app_user
POSTGRES_PASSWORD=change_me
POSTGRES_DB=aquachain
POSTGRES_PORT=5432
DATABASE_URL=postgresql://app_user:${POSTGRES_PASSWORD}@postgres:5432/aquachain?schema=public

# pgAdmin
PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=change_me_admin

# Blockchain
SEPOLIA_RPC=https://sepolia.infura.io/v3/{{INFURA_PROJECT_ID}}
