# Example environment variables (no secrets)

# Backend
PORT=3000
CORS_ORIGIN=http://localhost:5173

# Database (PostgreSQL)
POSTGRES_USER=app_user
POSTGRES_PASSWORD=change_me
POSTGRES_DB=aquachain
POSTGRES_PORT=5432
DATABASE_URL=postgresql://app_user:${POSTGRES_PASSWORD}@postgres:5432/aquachain?schema=public

# pgAdmin
PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=change_me_admin

# Blockchain
SEPOLIA_RPC=https://sepolia.infura.io/v3/{{INFURA_PROJECT_ID}}
DEPLOYER_KEY={{DEPLOYER_PRIVATE_KEY}}   # do not commit real key
DEPLOYER_KEY={{DEPLOYER_PRIVATE_KEY}}   # do not commit real key